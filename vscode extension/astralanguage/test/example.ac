class program
{
	public main()
	{
		string str = "Hello from Astra via Console instance #2"
		

		Console console = new Console()

		console.message(str)

		return
	}
}

class Console
{
	public message(string str)
	{
		ptr vga_pointer
		vga_pointer = 0x200000

		byte color = 0x2f

		for (int i; i < str.length(); i = i + 1)
		{
			byte c = str.get(i)

			vga_pointer.set(c)
			vga_pointer.shift(1)

			vga_pointer.set(color)
			vga_pointer.shift(1)
		}

		return
	}
}